{
  "name": "discord-player-youtube",
  "version": "0.8.1",
  "description": "A YouTube extractor for Discord Player.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "test:smoke": "npm run -s build && node scripts/smoke-youtube.mjs",
    "test:smoke:stream": "npm run -s build && cross-env NODE_OPTIONS=--openssl-legacy-provider SMOKE_STREAM=1 node scripts/smoke-youtube.mjs",
    "prepublishOnly": "npm run build",
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json"
  },
  "keywords": [
    "discord-player",
    "youtube",
    "extractor",
    "music",
    "discord"
  ],
  "author": "MangoD33",
  "license": "CC-BY-4.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MangoD33/discord-player-youtube.git"
  },
  "bugs": {
    "url": "https://github.com/MangoD33/discord-player-youtube"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.81",
    "bgutils-js": "^3.2.0",
    "discord-player": "^7.1.0",
    "dotenv": "^16.4.5",
    "googlevideo": "^4.0.4",
    "jsdom": "^27.0.1",
    "youtubei.js": "^16.0.1",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.38.31",
    "discord.js": "^14.24.0",
    "typescript": "^5.9.3"
  }
}
